@model ProgressViewModel

@{
    ViewBag.Title = Model.PageTitle;
}

<div>
    <h2>The Is the Progress View</h2>
</div>

<div class="container">
    <div class="row justify-content-md-center">
        <div class="col-5">
            <div class="progress" style="height: 40px;">
                <div class="progress-bar" role="progressbar" style="width: 60%;" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100">40%</div>
            </div>
        </div>
    </div>
</div>
